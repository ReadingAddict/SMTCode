	var infoWindow = new google.maps.InfoWindow;
	
	// function to manage the infoWindow layers
    function onMarkerClick(theMarker, textVal) 	{
    		infoWindow.close();
			infoWindow.setContent(textVal);
			infoWindow.open(theMarker.getMap(), theMarker);
    }
	
	// Create base marker icon
	var baseIcon_c0a802372732e9d49f5337e988c13bb1 = {
		size: new google.maps.Size(20, 34),
		anchor: new google.maps.Point(6, 20)
	};

	// create the base marker 'shadow' icon
	var iconShadow_c0a802372732e9d49f5337e988c13bb1 = {
		size: new google.maps.Size(37, 34),
		anchor: new google.maps.Point(6, 20),
		url: '//maps.gstatic.com/mapfiles/shadow50.png'
	};
	
	//  Creates a marker at the given point with the given text label
	function createMarker_c0a802372732e9d49f5337e988c13bb1(myMap, position, textVal, letterIndex, openWin, url) {
		// Create a lettered icon for this point using our icon class
		var letter = String.fromCharCode("A".charCodeAt(0) + letterIndex);
		
		// create marker icon value object.
		var markerIcon = {
			url: '//www.google.com/mapfiles/marker' + letter + '.png',
			size: baseIcon_c0a802372732e9d49f5337e988c13bb1.size,
			anchor: baseIcon_c0a802372732e9d49f5337e988c13bb1.anchor
		};

		// set our marker options
		var markerOptions = {
			visible: true,
			position: position,
			icon: markerIcon,
			shadow: iconShadow_c0a802372732e9d49f5337e988c13bb1,
			map: myMap
		};
		
		// create the actual marker
		var marker = new google.maps.Marker(markerOptions);
		
		// add a listener for a marker click event
		google.maps.event.addListener(marker, "click", function() {
			if (openWin == 1) {
				openWindow(url);
			} else if (openWin == 2) {
				window.location = url;
			} else if (openWin == 3) {
				eval(url);
			} else {
				onMarkerClick(marker, textVal);
			}
		});
		return marker;
	}
	
	// 
	function openWindow(loc) {
	    Shadowbox.open({
	        content:    loc,
			player:     "iframe",
	        title:      "Welcome"

	    });
	}
	
    function load_c0a802372732e9d49f5337e988c13bb1() {
		var uniqueId = 'map_' + 'c0a802372732e9d49f5337e988c13bb1';
		var myMapOptions = {
			panControl: true,
			streetViewControl: false,
			scrollwheel: false,
			// Zoom is 'on' by default, remove if not requested
			
			// Map, Satellite, Hybrid controls are 'on' by default, remove if not requested
			
			// mapTypeId is REQUIRED in order for map to be visible.
			mapTypeId: google.maps.MapTypeId.ROADMAP	
		};
	
		var myMap = new google.maps.Map(document.getElementById(uniqueId), myMapOptions);
		google.maps.event.addListener(myMap, "click", function() {
			infoWindow.close()
		});

		
				var sw = new google.maps.LatLng(39.892517, -105.1142011);
				var ne = new google.maps.LatLng(41.1771938, -85.1462907);
				var bounds = new google.maps.LatLngBounds(sw,ne);
				
				// make the map fit the bounds
				myMap.fitBounds(bounds);
				// now set the zoom level
				var bZoomLvl = myMap.getZoom();
				
				if (bZoomLvl == 21) {
					if (false) {
						if (! isNaN(parseInt('0', 10))) {
							bZoomLvl = bZoomLvl - parseInt('0', 10);
						}
					}
				}
				myMap.setCenter(bounds.getCenter(), (bZoomLvl));
	    	
       	var latLng_1 = new google.maps.LatLng(41.1771938, -85.1462907);
        
		var text_1 = "<b>Indiana Office</b><br/>10315 Dawsonâ€™s Creek Blvd. Suite A<br/>Fort Wayne, IN 46825";
		
        var marker_1 = createMarker_c0a802372732e9d49f5337e988c13bb1(myMap, latLng_1, text_1, 0, 0,'');
            
        
       	var latLng_2 = new google.maps.LatLng(39.892517, -105.1142011);
        
		var text_2 = "<b>Corporate Office</b><br/>11000 Westmoor Circle, Building 10 Suite 180<br/>Westminster, CO 80021<br/><br/>Get Directions:<br/><form action='//maps.google.com/maps' method='get' target='_blank' style='display:inline;'><input type='text' name='saddr' id='saddr' value='' /><input type='submit' value='Submit' /><input type='hidden' name='daddr' value='11000 Westmoor Circle, Building 10 Suite 180, Westminster CO 80021 US' /><input type='hidden' name='hl' value='en' /></form>";
		
        var marker_2 = createMarker_c0a802372732e9d49f5337e988c13bb1(myMap, latLng_2, text_2, 1, 0,'');
            
        
